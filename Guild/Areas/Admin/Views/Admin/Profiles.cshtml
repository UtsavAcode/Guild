@model List<Guild.Models.Domain.Worker>
<h1 class="mx-auto">Here are the list of all the users.</h1>
<table class="table">

    <tr>
        <th>Id</th>
        <th>FirstName</th>
        <th>LastName</th>
        <th>Email</th>
        <th>Address</th>
        <th>Phone</th>
        <th>Age</th>
       
    </tr>

    @foreach(var user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>@user.Address</td>
            <td>@user.Phone</td>
            <td>@user.Age</td>

        </tr>
    }

</table>