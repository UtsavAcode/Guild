@model List<Guild.Models.Register>

<div class="text-center">
    <h4 class="display-4">This is just a landing page (><)</h4>

    <h3>This is the list of users.</h3>
    <div class="container">
        <div class="row">
            <table class="table">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <th>Age</th>
                        <th>Password</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model != null)
                    {
                        @foreach (var worker in Model)
                        {
                            <tr>
                                <td>@worker.Name</td>
                                <td>@worker.Email</td>
                                <td>@worker.Phone</td>
                                <td>@worker.Age</td>
                                <td>@worker.Password</td>
                            </tr>
                        }

                    }
                </tbody>
            </table>
        </div>
    </div>   
</div>
